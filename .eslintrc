{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "window": true,
    "define": true,
    "require": true,
    "module": true,
    "goog": true
  },
  "extends": "google",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "es6": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "babel",
    "react",
    "emotion"
  ],
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "max-len": [
      "error",
      150,
      {
        "ignoreStrings": true
      }
    ],
    "react/jsx-boolean-value": 0,
    "react/jsx-closing-bracket-location": 1,
    "react/jsx-curly-spacing": [
      2,
      "always"
    ],
    "react/jsx-indent-props": [
      1,
      2
    ],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-wrap-multilines": 1,
    "react/react-in-jsx-scope": 1,
    "react/prefer-es6-class": 1,
    "react/jsx-no-bind": 0,
    "require-jsdoc": 0,
    "object-curly-spacing": [
      "error",
      "always"
    ]
  }
}